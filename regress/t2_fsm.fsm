precondition-function-args none
transition-function-args old-state,ctx

state T1a
	initial-state
	on-event T1_DONE -> T2
state T1b
	initial-state
	on-event T1_DONE -> T2
state T2
	on-event T2_DONE -> T3
state T3
	on-event T3_DONE1 -> T2
	on-event T3_DONE2 -> T4
state T4

